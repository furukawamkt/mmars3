<p id="notice"><%= notice %></p>

<h1>Fares</h1>

<table>
  <thead>
    <tr>
      <th>Ent</th>
      <th>Ext</th>
      <th>Price</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @fares.each do |fare| %>
      <tr>
        <td><%= fare.ent %></td>
        <td><%= fare.ext %></td>
        <td><%= fare.price %></td>
        <td><%= link_to 'Show', fare %></td>
        <td><%= link_to 'Edit', edit_fare_path(fare) %></td>
        <td><%= link_to 'Destroy', fare, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Fare', new_fare_path %>
